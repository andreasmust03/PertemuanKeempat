import java.util.Arrays;

public class ArraySatuDimensiMutable {
    public static void main(String[] args) {
        String[] names = {"Andreas", "Erik", "Faisa", "Gesang"};

        System.out.println(Arrays.toString(names));

        String erik = names[2];
        System.out.println(erik);

        int panjangArrayNames = names.length;
        System.out.println("Panjang array names adalah : " + panjangArrayNames);

        for (int i = 0; i < names.length; i++) {
            System.out.println("Nilai dari indeks ke- " + i + " adalah " + names[i]);
        }

    }
}
